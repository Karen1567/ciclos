Algoritmo notas
	definir a como entero
	escribir "ingrese el numero de notas"
	leer a
	Dimension noas[a]
	definir nota como real 
	para i=1 hasta a
		escribir "ingrese la nota numero ",i
		leer nota
		noas[i]=nota
	FinPara
	para i=1 hasta a  
		escribir "nota numero " i, " : " noas[i]
		
	FinPara
	
FinAlgoritmo
